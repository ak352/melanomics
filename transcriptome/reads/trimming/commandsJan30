mkdir /work/projects/melanomics/analysis/transcriptome/patient_2/adapter_quality_filter_trim/;cp -f /work/projects/melanomics/analysis/transcriptome/patient_2/FastQC/120827_SN386_0256_BC16NGACXX_RNA_4_PM_CCGTCC_L005.ft.R1_1.fastq /scratch/users/akrishna/.; /work/projects/isbsequencing/tools/cutadapt-1.2.1/bin/cutadapt -O 10 -b GATCGGAAGAGCACACGTCTGAACTCCAGTCACCCGTCCCGATCTCGTAT /scratch/users/akrishna/120827_SN386_0256_BC16NGACXX_RNA_4_PM_CCGTCC_L005.ft.R1_1.fastq > /scratch/users/akrishna/120827_SN386_0256_BC16NGACXX_RNA_4_PM_CCGTCC_L005.ft.R1_1.fastq.noadapter; ln -fs /work/projects/melanomics/analysis/transcriptome/patient_2/FastQC/120827_SN386_0256_BC16NGACXX_RNA_4_PM_CCGTCC_L005.ft.R2_2.fastq /scratch/users/akrishna/120827_SN386_0256_BC16NGACXX_RNA_4_PM_CCGTCC_L005.ft.R2_2.fastq.noadapter; perl /mnt/nfs/projects/melanomics/tools/trim-fastq.pl --input1 /scratch/users/akrishna/120827_SN386_0256_BC16NGACXX_RNA_4_PM_CCGTCC_L005.ft.R1_1.fastq.noadapter --input2 /scratch/users/akrishna/120827_SN386_0256_BC16NGACXX_RNA_4_PM_CCGTCC_L005.ft.R2_2.fastq.noadapter --output /scratch/users/akrishna/120827_SN386_0256_BC16NGACXX_RNA_4_PM_CCGTCC_L005.ft.R1_1.fastq.noadapter.quality_trimmed --quality-threshold 20 --fastq-type sanger --min-length 40 >/scratch/users/akrishna/120827_SN386_0256_BC16NGACXX_RNA_4_PM_CCGTCC_L005.ft.R1_1.fastq.noadapter.qsub; cp -f /scratch/users/akrishna/120827_SN386_0256_BC16NGACXX_RNA_4_PM_CCGTCC_L005.ft.R1_1.fastq.noadapter.quality_trimmed* /work/projects/melanomics/analysis/transcriptome/patient_2/adapter_quality_filter_trim//.; cp -f /scratch/users/akrishna/120827_SN386_0256_BC16NGACXX_RNA_4_PM_CCGTCC_L005.ft.R1_1.fastq.noadapter.qsub /work/projects/melanomics/analysis/transcriptome/patient_2/adapter_quality_filter_trim//.;
